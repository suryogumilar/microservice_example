<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<jdbcConnection
			connectionURL="${mybatis.connectionUrl}"
			driverClass="${mybatis.driverClass}" password="${mybatis.password}" userId="${mybatis.user}" />
		<javaModelGenerator
			targetPackage="${mybatis.javamodel}" targetProject="src/main/java" />
		<sqlMapGenerator targetPackage="${mybatis.mapper}"
			targetProject="src/main/resources" />
		<javaClientGenerator
			targetPackage="${mybatis.mapper}" targetProject="src/main/java"
			type="XMLMAPPER" />
		<table schema="${mybatis.schemadb}" tableName="%">

		</table>

	</context>
</generatorConfiguration>